<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<script>
	$(function(){
		
		//1. input태그의 속성이름은 type 속성값은 text인 것을 지정...  값 넣기. 
		$("input[type=text]").val("Hello jQuery") 
		
		$("input:text").val("Hellu?")
		$(":text").val("JQuery!")
		
		
/*
 
값을 받아오는 형식,
1. var x = $("input[type=text]").val()
	
값을 넣는 형식
2.  $("input[type=text]").val("value you want")
*/

		});
</script>


